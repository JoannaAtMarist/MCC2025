<!doctype html>
<html lang="en">
<head>
  <base href="{{ '/' | url }}"> <!-- a <base> tag so other relative links “just work” -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ title or "Marist Computing Conference" }}</title>
  <link rel="stylesheet" href="{{ '/assets/styles.css' | url }}">
  <link rel="stylesheet" href="{{ '/assets/logo-nav.css' | url }}">
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600;700&family=Source+Serif+4:wght@400;600&display=swap" rel="stylesheet">
  <meta name="description" content="Marist Computing Conference — November 7, 2025 — Murray Student Center">

  {% if site.is_preview %}<meta name="robots" content="noindex, nofollow">{% endif %}


</head>
<body>

  <a href="#main" class="skip-link">Skip to content</a>

  <!-- Row 1: Logo + Nav -->
  <div class="logo-bar">
    <div class="container logo-nav">
      <img src="/MCC2025/assets/marist-logo.jpg" alt="Marist University logo" class="logo-img">
      <nav class="top-nav">
        <a href="/MCC2025/">Home</a>
        <a href="/MCC2025/program/">Program</a>
        <a href="/MCC2025/venue/">Venue</a>
        <a href="/MCC2025/about/">About</a>
      </nav>
    </div>
  </div>


  <!-- Row 2: Text-only banner -->
  <header class="text-banner">
    <div class="container">
      <h1>Marist Computing Conference</h1>
      <h2 class="banner-sub">Friday, November 7, 2025 · Murray Student Center · Marist University · 3399 North Rd, Poughkeepsie, NY</h2>
    </div>
  </header>



  <main id="main" class="container">
    {{ content | safe }}
  </main>

  <footer class="site-footer">
    <div class="container">
      <p>&copy; 2025 Marist Computing Conference</p>
    </div>
  </footer>

</body>
</html>